all:
	cp ../lib_x86_64/sunvox.so ./
	cp ../../resources/* ./
	gcc -m64 -g -c test1.cpp
	gcc -m64 -g -c test2.cpp
	gcc -m64 -g -c test3.cpp
	gcc -m64 -g -c test4.cpp
	gcc -m64 -g -c test5.cpp
	gcc -m64 -g -c test6.cpp
	gcc -m64 -o test1 test1.o -lstdc++ -ldl -lm
	gcc -m64 -o test2 test2.o -lstdc++ -ldl -lm
	gcc -m64 -o test3 test3.o -lstdc++ -ldl -lm
	gcc -m64 -o test4 test4.o -lstdc++ -ldl -lm
	gcc -m64 -o test5 test5.o -lstdc++ -ldl -lm
	gcc -m64 -o test6 test6.o -lstdc++ -ldl -lm

clean:
	rm -f *.o *.so test1 test2 test3 test4 test5 test6 *.sunvox *.sunsynth *.xi *.wav
